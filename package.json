{
  "name": "zarm-web",
  "version": "0.0.1-alpha.13",
  "description": "基于 React 的桌面端UI库",
  "keywords": [
    "react",
    "react-ui",
    "react-component",
    "dragon-ui",
    "zarm-web",
    "component",
    "components",
    "ui",
    "framework",
    "frontend",
    "web"
  ],
  "bugs": {
    "url": "https://github.com/JeromeLin/zarm-web"
  },
  "repository": "git://github.com/JeromeLin/zarm-web",
  "license": "MIT",
  "sideEffects": [
    "dist/*",
    "components/style/*",
    "components/**/style/*",
    "es/**/style/*",
    "lib/**/style/*",
    "site/**/**.scss"
  ],
  "main": "lib/index.js",
  "module": "es/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "clean": "rimraf types lib es dist native assets coverage",
    "dev": "zarm dev -p 3001",
    "build:es": "rimraf es && tsc --outDir es -d --emitDeclarationOnly && zarm build --mode es --path components --out-dir es --copy-files --build-css",
    "build:lib": "rimraf lib && tsc --outDir lib -d --emitDeclarationOnly && zarm build --mode lib --path components --out-dir lib --copy-files --build-css",
    "build:umd": "rimraf dist && zarm build --mode umd --path components/style/entry.tsx,components/index.tsx --out-dir dist && zarm build --mode umd-ugly --path components/style/entry.tsx,components/index.tsx --out-dir dist",
    "build": "npm run build:lib && npm run build:es && npm run build:umd",
    "deploy:build": "rimraf assets && mkdir assets && touch assets/CNAME && echo 'web.zarm.design' > assets/CNAME && zarm deploy",
    "deploy": "npm run deploy:build -- -p && gh-pages -d assets",
    "lint:tsc": "tsc --noEmit",
    "lint:ts": "eslint --ext .ts,.tsx components",
    "lint:js": "eslint --ext .js,.jsx components site",
    "lint:style": "stylelint \"components/**/*.scss\" \"site/**/*.scss\" --syntax scss",
    "lint": "npm run lint:tsc && npm run lint:ts && npm run lint:js && npm run lint:style",
    "test": "rimraf coverage && zarm test -s scripts/jest/setup.js -c",
    "coverage": "cat ./coverage/lcov.info | coveralls",
    "authors": "git log --format='%aN <%aE>' | sort -u | grep -v 'users.noreply.github.com' > AUTHORS",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -w -r 0",
    "release": "bash scripts/release.sh",
    "prettier": "prettier -c --write '**/*'",
    "pretty-quick": "pretty-quick",
    "bundlesize": "bundlesize",
    "sort": "npx sort-package-json",
    "install-react-16": "yarn add react@16 react-dom@16 enzyme-adapter-react-16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS"
    }
  },
  "browserslist": [
    "last 2 versions",
    "Firefox ESR",
    "> 1%",
    "ie >= 9",
    "iOS >= 7",
    "Android >= 4"
  ],
  "dependencies": {
    "async-validator": "^1.10.1",
    "classnames": "2.2.6",
    "color": "^3.1.0",
    "css-animation": "^1.2.5",
    "hoist-non-react-statics": "^3.3.2",
    "normalize.css": "8.0.1",
    "rc-animate": "^3.0.0",
    "zarm": "2.8.0"
  },
  "devDependencies": {
    "@babel/standalone": "7.12.15",
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@sentry/browser": "^6.2.3",
    "@testing-library/react": "^11.2.3",
    "@types/classnames": "^2.2.3",
    "@types/color": "^3.0.1",
    "@types/enzyme": "^3.10.8",
    "@types/jest": "^26.0.20",
    "@types/lodash": "^4.14.151",
    "@types/node": "^14.0.11",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "3.10.1",
    "@typescript-eslint/parser": "3.10.1",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.5.0",
    "@zarm-design/cli": "0.0.36",
    "babel-eslint": "10.1.0",
    "babel-plugin-import": "^1.13.0",
    "babel-plugin-prismjs": "^2.0.1",
    "browserslist": "^4.6.6",
    "bundlesize": "^0.18.1",
    "chalk": "^4.1.0",
    "change-case": "^4.1.1",
    "concurrently": "^6.0.0",
    "conventional-changelog-cli": "^2.0.34",
    "core-js": "^3.0.1",
    "coveralls": "^3.0.9",
    "cross-env": "^7.0.2",
    "dekko": "^0.2.1",
    "docsearch.js": "^2.6.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.6.1",
    "eslint": "7.10.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-config-za": "2.2.2",
    "eslint-plugin-babel": "5.3.1",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "4.1.2",
    "eslint-plugin-vue": "7.1.0",
    "gh-pages": "^3.1.0",
    "history": "^5.0.0",
    "husky": "^4.3.8",
    "is-wsl": "^2.1.0",
    "istanbul-api": "^3.0.0",
    "lint-staged": "^4.3.0",
    "marked": "^2.0.0",
    "prettier": "2.2.1",
    "pretty-quick": "^3.1.0",
    "prismjs": "^1.23.0",
    "prop-types": "^15.6.2",
    "qrious": "^4.0.2",
    "raw-loader": "^4.0.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.0.0",
    "react-intl": "^5.12.1",
    "react-loadable": "^5.5.0",
    "react-router-dom": "^5.1.2",
    "rimraf": "^3.0.2",
    "stylelint": "13.8.0",
    "stylelint-checkstyle-formatter": "0.1.2",
    "stylelint-config-za": "0.3.0",
    "stylelint-scss": "^3.18.0",
    "typescript": "4.0.2"
  },
  "peerDependencies": {
    "react": ">=16.3.1",
    "react-dom": ">=16.3.1"
  },
  "bundlesize": [
    {
      "path": "./dist/zarm-web.min.js",
      "maxSize": "100 kB"
    },
    {
      "path": "./dist/zarm-web.min.css",
      "maxSize": "30 kB"
    }
  ],
  "tags": [
    "zarm-web",
    "react",
    "zhongan"
  ]
}
